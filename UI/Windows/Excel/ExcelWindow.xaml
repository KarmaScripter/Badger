<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Badger" xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        x:Class="Badger.ExcelWindow" mc:Ignorable="d" Title="Excel Data" Height="730" Width="1350"
        Background="#FF141414" BorderBrush="#FF0078D4" FontSize="9" Foreground="#FF6ABDFC"
        WindowStartupLocation="CenterScreen" Icon="/UI/Windows/Excel/Application.png">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="443*" />
            <ColumnDefinition Width="7*" />
        </Grid.ColumnDefinitions>
        <syncfusion:SfSpreadsheetRibbon x:Name="Ribbon" HorizontalAlignment="Left" Margin="27,35,0,0"
                                        VerticalAlignment="Top" Width="1292" Height="122" />
        <local:ToolStrip x:Name="Toolbar" Margin="10,656,10,13" Grid.ColumnSpan="2">
            <syncfusion:MenuItemSeparator />
            <local:ToolStripLabel />
            <syncfusion:MenuItemSeparator />
            <local:ToolStripComboBox />
            <syncfusion:MenuItemSeparator />
            <local:ToolStripTextBox />
            <syncfusion:MenuItemSeparator />
            <local:ToolStripButton x:Name="FirstButton"
                                   ImageSource="/UI/Windows/Excel/FirstButton.png" />
            <local:ToolStripButton x:Name="PreviousButton"
                                   ImageSource="/UI/Windows/Excel/PreviousButton.png" />
            <local:ToolStripButton x:Name="NextButton"
                                   ImageSource="/UI/Windows/Excel/NextButton.png" />
            <local:ToolStripButton x:Name="LastButton"
                                   ImageSource="/UI/Windows/Excel/LastButton.png" />
            <local:ToolStripButton x:Name="LookupButton"
                                   ImageSource="/UI/Windows/Excel/LookUpButton.png" />
            <local:ToolStripButton x:Name="RefreshButton"
                                   ImageSource="/UI/Windows/Excel/RefreshButton.png" />
            <local:ToolStripButton x:Name="EditButton"
                                   ImageSource="/UI/Windows/Excel/EditButton.png" />
            <local:ToolStripButton x:Name="DeleteButton"
                                   ImageSource="/UI/Windows/Excel/DeleteButton.png" />
            <local:ToolStripButton x:Name="SaveButton"
                                   ImageSource="/UI/Windows/Excel/SaveButton.png" />
            <local:ToolStripButton x:Name="ExportButton"
                                   ImageSource="/UI/Windows/Excel/ExportButton.png" />
            <local:ToolStripButton x:Name="BrowseButton"
                                   ImageSource="/UI/Windows/Excel/BrowseButton.png" />
            <local:ToolStripButton x:Name="MenuButton"
                                   ImageSource="/UI/Windows/Excel/MenuButton.png" />
        </local:ToolStrip>
        <syncfusion:SfSpreadsheet x:Name="Spreadsheet" HorizontalAlignment="Left" Margin="27,162,0,0"
                                  VerticalAlignment="Top" Height="488" Width="1292" />
    </Grid>
</Window>
